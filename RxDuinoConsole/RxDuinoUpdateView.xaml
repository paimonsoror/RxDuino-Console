<Window x:Class="RxDuinoConsole.RxDuinoUpdateView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Update Firmware..." Height="76" Width="574" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid Background="WhiteSmoke">
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="fileBox" VerticalAlignment="Top" Width="368" />
        <Button Content="Browse" Margin="0,10,93,0" Name="browseButton" Height="25" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="browseButton_Click" />
        <Button Content="Update" Margin="0,10,12,0" Name="updateButton" HorizontalAlignment="Right" Width="75" Height="25" VerticalAlignment="Top" />
    </Grid>
</Window>
